<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TelaLogin.pgPrincipal"
             Title="pgPrincipal">
    <StackLayout>
        <Button Text="Voltar"
                x:Name="btnVoltar"
                Clicked="btnVoltar_Clicked"/>

        <Frame BackgroundColor="Black"
               CornerRadius="10">
            <StackLayout>
                <Label x:Name="txtNome"/>
                <Label x:Name="txtLogin"/>
                <Label x:Name="txtSenha"/>
                <Label x:Name="txtEmail"/>
                <Label x:Name="txtIdade"/>
                <Image x:Name="imgPerfil"/>
            </StackLayout>
        </Frame>
    </StackLayout>
</ContentPage>